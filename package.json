{
  "name": "next-starter",
  "description": "Next.js starter with TypeScript, Tailwind CSS, Storybook, Cypress, and MSW",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ./src",
    "gen": "plop",
    "cy:open": "cypress open",
    "cy:run": "NODE_ENV=test cypress run",
    "cy:run:ct": "NODE_ENV=test cypress run --component --spec='src/**/*'",
    "cy:run:e2e": "NODE_ENV=test cypress run --e2e --spec='cypress/e2e/**/*'",
    "chromatic": "chromatic",
    "sb:dev": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.[ts,tsx,js,jsx]": "eslint --cache --fix",
    "*./src": "prettier --write"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.1",
    "class-variance-authority": "^0.6.0",
    "framer-motion": "^6",
    "next": "13.4.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@mswjs/data": "^0.12.0",
    "@next/eslint-plugin-next": "^13.4.3",
    "@storybook/addon-actions": "^7.0.12",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/cli": "^7.0.12",
    "@storybook/nextjs": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/node": "^16",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "chromatic": "^6.17.4",
    "cypress": "^12.12.0",
    "eslint": "^8",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "msw": "^1.2.1",
    "msw-storybook-addon": "^1.8.0",
    "plop": "^3.1.2",
    "postcss": "^8.4.23",
    "prettier": "^2",
    "storybook": "^7.0.12",
    "tailwindcss": "^3.3.2",
    "typescript": "4.8.4"
  }
}
