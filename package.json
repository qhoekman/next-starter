{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:ssl": "next dev -p 3001 | local-ssl-proxy --source 3000 --target 3001 --key localhost-key.pem --cert localhost.pem",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "format": "prettier --write src/**/*",
    "gen": "plop",
    "cy:open": "cypress open",
    "cy:run": "NODE_ENV=test start-server-and-test dev 3000 'cypress run'",
    "cy:run:ct": "NODE_ENV=test cypress run --component --spec='src/**/*'",
    "cy:run:e2e": "NODE_ENV=test cypress run --e2e --spec='cypress/e2e/**/*'",
    "chromatic": "chromatic",
    "sb:dev": "storybook dev -p 6006 -s ./public",
    "sb:build": "storybook build -s ./public ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.3.1",
    "@radix-ui/react-slot": "^1.0.1",
    "@radix-ui/react-toast": "^1.1.4",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.6.1",
    "i18next": "^23.2.2",
    "next": "*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "react-i18next": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@cypress/webpack-dev-server": "^3.3.0",
    "@hookform/resolvers": "^2.9.11",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@mswjs/data": "^0.12.0",
    "@next/eslint-plugin-next": "^13.4.3",
    "@radix-ui/react-label": "^2.0.2",
    "@storybook/addon-a11y": "^7.0.24",
    "@storybook/addon-actions": "^7.0.24",
    "@storybook/addon-docs": "^7.0.24",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/cli": "^7.0.24",
    "@storybook/components": "^7.0.24",
    "@storybook/nextjs": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/testing-library": "^0.1.0",
    "@tailwindcss/forms": "^0.5.3",
